bool searchInBST(BinaryTreeNode<int> *root , int k) {
    if(root==NULL)
        return root;
    
    if(root->data== k)
        return true;

   
    if(root->data > k)
    return searchInBST(root->left , k);
        
    if( root->data < k )
    return searchInBST(root->right , k);
    // Write your code here
}